<!--顶部导航栏-->
<view style="width: 100%;height: 180rpx;display: flex;justify-content: center;background-color: rgb(255, 255, 255);position: fixed;z-index: 2;">
  <text style="margin-top: 100rpx;font-size: 40rpx;">商城</text>
</view>

<view style="background-color: {{Day ? '#EFEDEA' : 'black'}};margin-top: 180rpx;">

<!--搜索框-->
<view class="search-container">
  <image src="{{avatar}}" style="height: 80rpx;width: 80rpx;margin-right: 10rpx;border-radius: 50%;" mode="aspectFill" ></image>
  <input type="text" class="searchinput" placeholder="请输入搜索内容" bindinput="onSearchInput" bindconfirm="onsearch" adjust-position="{{false}}" hold-keyboard="{{true}}" cursor-spacing="150" />
  <view style="display: flex;align-items: center;">
    <text class="searchbutton" bind:tap="onsearch">搜索 </text>
  </view>
</view>
<!--Type筛选框-->
<view style="display: flex;justify-content: center;align-items: center;">
<view class="TypeFilterContainer">
  <checkbox-group class="group" bindchange="SelectType">
   <label  wx:for="{{AllType}}" wx:key="id">
    <checkbox class="TypeCheckBox0" value="{{item.name}}" checked="{{item.checked}}"/>
    <view class="TypeCheckBox" style="padding: 4rpx;border-radius: 50%;;">
      <view style="display: flex;flex-direction: column;align-items: center;">
        <image src="{{item.icon}}" style="height: 100rpx;width: 100rpx; background-color: {{item.checked?'#ffe79f': 'transparent'}};border-radius: 50%;"></image>
        <text class="TypeText">{{item.name}}</text>
      </view>
     </view>    
   </label>
  </checkbox-group>
</view>
</view>
