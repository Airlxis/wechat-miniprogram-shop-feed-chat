<view style="border-radius: 30rpx;display: flex;justify-content: space-around;">
  <text bind:tap="ToAddGoods" style="font-family: 'Type';background-color: #ffe79f;color: #ef8430;border:#ef8430 4rpx solid;border-radius: 40rpx;margin-top: 40rpx;padding: 50rpx;font-size: 40rpx;" >我也要卖</text>
  <text bind:tap="ApplyIP" style="font-family: 'Type';background-color: #ffe79f;color: #ef8430;border:#ef8430 4rpx solid;border-radius: 40rpx;margin-top: 40rpx;padding: 50rpx;font-size: 40rpx;" >申请新IP</text>
</view>

<!--输入框无内容-->
<view class="goodsform" wx:if="{{FilteredInput.length === 0}}" style="padding-top: 100rpx;margin-top: -40rpx;margin-bottom: 150rpx;">
  <view  class="singlegood"  wx:for="{{FilteredGoods}}"  wx:key="_id" data-id="{{item._id}}"  bindtap="goToGoodDetail" >
    <view style="background-color: white;width: 100%;display: flex;justify-content: center;border-radius: 10rpx;box-shadow: 0 4rpx 8rpx rgba(0, 0, 0, 0.1);  transition: transform 0.3s ease;">
      <image class="picture" mode="aspectFill" src="{{item.MainImageFileID}}"></image>
    </view>  
    <view style="display: flex;flex-direction: column;align-self: flex-start;">
      <text>{{item.MainIntroduction}}</text>
      <text style="font-size: 40rpx;">￥{{item.Price}}</text>
    </view>
  </view>

 </view>
<!--输入框有内容-->
<view class="goodsform" wx:else style="margin-bottom: 150rpx;">
  <view  class="singlegood"  wx:for="{{FilteredInput}}" wx:key="_id" data-id="{{item._id}}"  bindtap="goToGoodDetail" >
    <view style="background-color: white;width: 100%;display: flex;justify-content: center;border-radius: 10rpx;box-shadow: 0 4rpx 8rpx rgba(0, 0, 0, 0.1);  transition: transform 0.3s ease;">
      <image class="picture" mode="aspectFill" src="{{item.MainImageFileID}}"></image>
    </view>  
    <view style="display: flex;flex-direction: column;align-self: flex-start;">
      <text>{{item.MainIntroduction}}</text>
      <text style="font-size: 40rpx;">￥{{item.Price}}</text>
    </view>
  </view>
</view>

</view>

<!--导航栏-->
<view style="position: fixed;bottom: 0rpx;background-color: white;width: 100%;height: 150rpx;">
<view style="display: flex;justify-content: space-around;">
  <view style="display: flex;flex-direction: column;align-items: center;">
    <image src="/images/商城.png" style="height: 90rpx;width: 90rpx;" mode="aspectFill"></image>
    <text>商城</text>
  </view>
  
  <view style="display: flex;flex-direction: column;align-items: center;" bind:tap="ToSquare">
    <image src="/images/广场.png" style="height: 90rpx;width: 90rpx;" mode="aspectFill"></image>
    <text>广场</text>
  </view>

  <view style="display: flex;flex-direction: column;align-items: center;" bind:tap="ToMessage">
    <image src="/images/消息.png" style="height: 90rpx;width: 90rpx;" mode="aspectFill"></image>
    <text wx:if="{{GlobalUnread === 0}}">消息</text>
    <view wx:if="{{GlobalUnread !== 0}}" style="display: flex;justify-content: center;background-color: red;width: 40rpx;border-radius: 50%;">
      <text  style="color: white;">{{GlobalUnread}}</text>
    </view>  
  </view>

  <view style="display: flex;flex-direction: column;align-items: center;" bind:tap="ToMine">
    <image src="/images/我的.png" style="height: 90rpx;width: 90rpx;" mode="aspectFill"></image>
    <text>我的</text>
  </view>
</view>
</view>